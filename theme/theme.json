{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"typography": {
			"dropCap": false,
			"textTransform": false,
			"letterSpacing": false,
			"textDecoration": false,
			"fontStyle": false,
			"fontWeight": true,
			"lineHeight": false,
			"fontSizes": [
				{
					"name": "xs",
					"slug": "xs",
					"size": "0.75rem"
				},
				{
					"name": "sm",
					"slug": "sm",
					"size": "0.875rem"
				},
				{
					"name": "base",
					"slug": "base",
					"size": "1rem"
				},
				{
					"name": "lg",
					"slug": "lg",
					"size": "1.125rem"
				},
				{
					"name": "xl",
					"slug": "xl",
					"size": "1.25rem"
				},
				{
					"name": "2xl",
					"slug": "2xl",
					"size": "1.5rem"
				},
				{
					"name": "3xl",
					"slug": "3xl",
					"size": "1.875rem"
				},
				{
					"name": "4xl",
					"slug": "4xl",
					"size": "2.25rem"
				},
				{
					"name": "5xl",
					"slug": "5xl",
					"size": "3rem"
				},
				{
					"name": "6xl",
					"slug": "6xl",
					"size": "4rem"
				}
			],
			"fontFamilies": [
				{
					"name": "Bodoni Moda",
					"slug": "bodonimoda",
					"fontFamily": "Bodoni Moda, serif"
				},
				{
					"name": "Cormorant Garamond",
					"slug": "cormorantgaramond",
					"fontFamily": "Cormorant Garamond, serif"
				}
			]
		},
		"color": {
			"palette": [
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#000000",
					"name": "Foreground"
				},
				{
					"slug": "primary",
					"color": "#FFC6C6",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#D9D9D9",
					"name": "Secondary"
				}
			]
		},
		"layout": {
			"contentSize": "50rem",
			"wideSize": "80rem"
		}
	},
	"styles": {
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--6xl)",
					"fontWeight": "400"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3xl)",
					"fontWeight": "400"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)",
					"fontWeight": "400"
				}
			}
		}
	}
}
